{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Spring": {
    "application": {
      "name": "order-service"
    }
  },
  "Eureka": {
    "Client": {
      "ServiceUrl": "http://localhost:8761/eureka/" // URL del servidor Eureka
      // "shouldFetchRegistry": true,
      // "registerWithEureka": true,
      // "validateCertificates": false // Puede ser necesario en entornos de desarrollo
    },
    "Instance": {
      "hostname": "localhost", // El hostname que Eureka usará para tu servicio
      "port": 5202 // El puerto del servicio 
      // "preferIpAddress": true // Usa la dirección IP en lugar del nombre de host para registrar
    }
  }
}
